// JAVA'DSA


class Solution 
{
    public static int maximumProfit(int prices[]) 
    {
        int buy=prices[0];
        int profit=0;
        
        for(int i=1;i<prices.length;i++)
        {
            if(buy>=prices[i])
            {
                buy=prices[i];
            }
            else
            {
                profit=Math.max(profit,prices[i]-buy);
            }
        }
        return profit;
        
    }
}

